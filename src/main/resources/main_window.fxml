<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.MainWindowController">
   <children>
      <TableView fx:id="productTable" layoutX="-1.0" prefHeight="320.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="42.0" resizable="false" text="id" />
          <TableColumn fx:id="prodidColumn" prefWidth="183.0" text="prodid" />
            <TableColumn fx:id="titleColumn" prefWidth="262.0" text="title" />
            <TableColumn fx:id="priceColumn" minWidth="0.0" prefWidth="112.0" resizable="false" text="price" />
        </columns>
      </TableView>
      <Button fx:id="add" layoutX="14.0" layoutY="343.0" mnemonicParsing="false" onAction="#add" prefHeight="44.0" prefWidth="68.0" text="Insert" />
      <Button fx:id="delete" layoutX="90.0" layoutY="343.0" mnemonicParsing="false" onAction="#delete" prefHeight="44.0" prefWidth="68.0" text="Delete" />
      <Button fx:id="showAll" layoutX="165.0" layoutY="343.0" mnemonicParsing="false" onAction="#showAll" prefHeight="44.0" prefWidth="68.0" text="Show all" />
      <Button layoutX="240.0" layoutY="343.0" mnemonicParsing="false" onAction="#find" prefHeight="44.0" prefWidth="68.0" text="Find" />
      <Button layoutX="314.0" layoutY="343.0" mnemonicParsing="false" onAction="#edit" prefHeight="44.0" prefWidth="68.0" text="Edit" />
      <Button layoutX="388.0" layoutY="343.0" mnemonicParsing="false" onAction="#filter" prefHeight="44.0" prefWidth="68.0" text="Filter" />
   </children>
</AnchorPane>
